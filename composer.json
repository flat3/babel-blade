{
  "name": "flat3/babel-blade",
  "description": "Blade support for Babel scripts",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Lloyd",
      "email": "chrislloyd403@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "symfony/process": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "Flat3\\Blade\\Babel\\": "src/",
      "Flat3\\Blade\\Babel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Flat3\\Blade\\Babel\\ServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4|^9.0",
    "orchestra/testbench": "^6.0",
    "spatie/phpunit-snapshot-assertions": "^4.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
